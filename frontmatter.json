{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.snippets": {
    "Image snippet": {
      "description": "src/title/alt/width/height",
      "body": "<img src=\"[[&mediaUrl]]\" title=\"[[title]]\" alt=\"[[alt]]\" width=\"[[mediaWidth]]\" height=\"[[mediaHeight]]\" >",
      "isMediaSnippet": true
    },
    "anchor image": {
      "description": "imageをaタグで優しく包み込みます。",
      "body": "<a href=\"\" target=\"_blank\"><img src=\"[[mediaUrl]]\" alt=\"[[alt]]\" title=\"[[title]]\" width=\"[[mediaWidth]]\" height=\"[[mediaHeight]]\" ></a>",
      "isMediaSnippet": true
    },
    "<br/>": {
      "description": "改行",
      "body": "<br/>",
      "fields": []
    }
  },
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "filePrefix": null,
      "path": "[[workspace]]/src/content/blog",
      "previewPath": "/posts/"
    },
    {
      "title": "about",
      "path": "[[workspace]]/src/pages/about"
    }
  ],
  "frontMatter.git.enabled": true,
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "featured",
          "name": "featured",
          "type": "boolean"
        },
        {
          "title": "pubDatetime",
          "name": "pubDatetime",
          "default": "{{now}}",
          "type": "datetime"
        },
        {
          "title": "modDatetime",
          "name": "modDatetime",
          "type": "datetime"
        },
        {
          "title": "postSlug",
          "name": "postSlug",
          "type": "string"
        }
      ]
    }
  ]
}
